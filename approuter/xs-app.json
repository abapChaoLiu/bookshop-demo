{
  "welcomeFile": "/bookshopdemoapp/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "^/keyuser/(.*)$",
      "target": "$1",
      "service": "com.sap.ui.flex.cf.keyuser",
      "endpoint": "keyuser"
    },
    {
      "source": "/api/(.*)$",
      "target": "/api/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "/v2standalone/admin/(.*)$",
      "target": "/v2/admin/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api_v2",
      "csrfProtection": true
    },
    {
      "source": "/v2standalone/catalog/(.*)$",
      "target": "/v2/catalog/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api_v2",
      "csrfProtection": true
    },
    {
      "source": "/v2/admin/(.*)$",
      "target": "/v2/admin/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "/v2/catalog/(.*)$",
      "target": "/v2/catalog/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "/admin/(.*)$",
      "target": "/admin/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "/catalog/(.*)$",
      "target": "/catalog/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "/public/(.*)$",
      "target": "/public/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    }
  ]
}
